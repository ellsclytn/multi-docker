{
  "awsebdockerrunversion": 2,
  "containerdefinitions": [
    {
      "name": "client",
      "image": "ellsclytn/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "ellsclytn/multi-server",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "ellsclytn/multi-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "ellsclytn/multi-nginx",
      "essential": true,
      "memory": 128,
      "portmappings": [
        {
          "hostport": 80,
          "containerport": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}
